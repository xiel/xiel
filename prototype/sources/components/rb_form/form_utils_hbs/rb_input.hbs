{{#uniqueID}}
	{{#contains "checkbox|radio" type}}

		{{> rb_input_state_btn_wrapper_top }}
		{{> rb_input_input }}
		{{> rb_input_state_btn_wrapper_bottom }}

	{{else}}

		{{> rb_input_wrapper_top }}

		{{#is type "select"}}
			{{> rb_select }}
		{{else}}
			{{#is type "textarea"}}
				{{> rb_textarea }}
			{{else}}
				{{> rb_input_input }}
			{{/is}}
		{{/is}}


		{{> rb_input_wrapper_bottom }}
	{{/contains}}
{{/uniqueID}}






