{
  "name": "xiel-website",
  "private": true,
  "description": "Portfolio and Blog",
  "version": "0.1.0",
  "author": "Felix Leupold <felix@xiel.de>",
  "dependencies": {
    "core-js": "3.2.1",
    "embla-carousel": "4.0.6",
    "embla-carousel-wheel-gestures": "1.0.6",
    "polished": "3.4.1"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn cleanup; yarn gql-generate; yarn type-check",
    "build": "gatsby build",
    "develop": "gatsby develop --host 0.0.0.0",
    "gql-generate": "graphql-codegen --config graphql-codegen.yml",
    "gql-generate:watch": "yarn gql-generate --watch",
    "format": "prettier --write src/**/*.{js,jsx,json,ts,tsx}",
    "start": "yarn cleanup; yarn gql-generate; concurrently 'yarn type-check:watch' 'yarn develop'",
    "serve": "gatsby serve",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "test": "react-scripts test",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check --watch",
    "cleanup": "rm -rf .cache"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xiel/xiel"
  },
  "bugs": {
    "url": "https://github.com/xiel/xiel/issues"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "7.16.7",
    "@emotion/core": "10.0.17",
    "@emotion/styled": "10.0.17",
    "@graphql-codegen/add": "1.7.0",
    "@graphql-codegen/cli": "1.7.0",
    "@graphql-codegen/typescript-operations": "1.7.0",
    "@loadable/babel-plugin": "5.13.2",
    "@loadable/component": "5.15.2",
    "@loadable/server": "5.14.0",
    "@loadable/webpack-plugin": "5.7.1",
    "@testing-library/react": "9.5.0",
    "@types/jest": "26.0.24",
    "@types/loadable__component": "5.10.0",
    "@types/node": "14.14.10",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-helmet": "5.0.20",
    "@types/throttle-debounce": "2.1.0",
    "concurrently": "4.1.2",
    "emotion-theming": "10.0.17",
    "eslint-config-prettier": "6.2.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-app": "6.0.1",
    "gatsby": "2.15.9",
    "gatsby-image": "2.2.17",
    "gatsby-plugin-emotion": "4.1.22",
    "gatsby-plugin-manifest": "2.2.14",
    "gatsby-plugin-offline": "3.0.3",
    "gatsby-plugin-react-helmet": "3.1.6",
    "gatsby-plugin-react-svg": "3.0.0",
    "gatsby-plugin-sharp": "2.2.20",
    "gatsby-plugin-typescript": "2.1.6",
    "gatsby-plugin-typescript-checker": "1.1.1",
    "gatsby-source-filesystem": "2.1.21",
    "gatsby-transformer-json": "2.2.7",
    "gatsby-transformer-sharp": "2.2.13",
    "husky": "3.0.5",
    "i18next": "17.0.13",
    "normalize.css": "8.0.1",
    "prettier": "2.2.1",
    "pretty-quick": "1.11.1",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.13.0",
    "react-helmet": "5.2.1",
    "react-i18next": "10.12.2",
    "react-scripts": "3.1.1",
    "react-spring": "8.0.27",
    "resize-observer-polyfill": "1.5.1",
    "throttle-debounce": "2.1.0",
    "ts-node": "8.3.0",
    "typescript": "4.0.3",
    "use-location-state": "2.1.5"
  },
  "resolutions": {
    "eslint": "6.1.0",
    "babel-eslint": "10.1.0",
    "webpack": "4.39.1"
  }
}
