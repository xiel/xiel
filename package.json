{
  "name": "xiel-website",
  "private": true,
  "description": "Portfolio and Blog",
  "version": "0.1.0",
  "author": "Felix Leupold <felix@xiel.de>",
  "dependencies": {
    "core-js": "^3.2.1",
    "gh-pages": "^2.1.1",
    "polished": "^3.4.1"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn cleanup; yarn gql-generate; yarn type-check",
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "develop": "gatsby develop --host 0.0.0.0",
    "gql-generate": "graphql-codegen --config graphql-codegen.yml",
    "gql-generate:watch": "yarn gql-generate --watch",
    "format": "prettier --write src/**/*.{js,jsx,json,ts,tsx}",
    "start": "yarn cleanup; yarn gql-generate; concurrently 'yarn type-check:watch' 'yarn develop'",
    "serve": "gatsby serve",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "test": "react-scripts test",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check --watch",
    "cleanup": "rm -rf .cache; rm -rf node_modules/gh-pages/.cache"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xiel/xiel"
  },
  "bugs": {
    "url": "https://github.com/xiel/xiel/issues"
  },
  "devDependencies": {
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "@graphql-codegen/add": "^1.6.1",
    "@graphql-codegen/cli": "^1.2.0",
    "@graphql-codegen/typescript-operations": "^1.2.0",
    "@loadable/babel-plugin": "^5.10.0",
    "@loadable/component": "^5.10.2",
    "@loadable/server": "^5.10.2",
    "@loadable/webpack-plugin": "^5.7.1",
    "@testing-library/react": "^9.1.4",
    "@types/jest": "^24.0.15",
    "@types/loadable__component": "^5.10.0",
    "@types/node": "^12.0.7",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "@types/throttle-debounce": "^2.1.0",
    "concurrently": "^4.1.2",
    "emotion-theming": "^10.0.14",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-app": "^6.0.1",
    "gatsby": "^2.8.6",
    "gatsby-image": "^2.1.2",
    "gatsby-plugin-emotion": "^4.1.0",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-offline": "^3.0.3",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-react-svg": "^2.1.2",
    "gatsby-plugin-sharp": "^2.1.3",
    "gatsby-plugin-typescript": "^2.1.6",
    "gatsby-plugin-typescript-checker": "^1.1.1",
    "gatsby-source-filesystem": "^2.0.38",
    "gatsby-transformer-json": "^2.1.11",
    "gatsby-transformer-sharp": "^2.1.21",
    "husky": "^3.0.5",
    "i18next": "^17.0.3",
    "normalize.css": "^8.0.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-i18next": "^10.11.0",
    "react-scripts": "^3.0.1",
    "react-spring": "^8.0.27",
    "resize-observer-polyfill": "^1.5.1",
    "throttle-debounce": "^2.1.0",
    "ts-node": "^8.2.0",
    "typescript": "^3.6.2",
    "use-location-state": "2.1.5"
  },
  "resolutions": {
    "eslint": "6.1.0",
    "babel-eslint": "10.0.2",
    "webpack": "4.39.1"
  }
}
