{
  "name": "xiel-website",
  "private": true,
  "description": "Portfolio and Blog",
  "version": "0.1.0",
  "author": "Felix Leupold <felix@xiel.de>",
  "dependencies": {
    "emotion-theming": "^10.0.14",
    "gatsby-plugin-react-svg": "^2.1.2",
    "react-spring": "^8.0.27"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "prebuild": "yarn gql-generate; yarn type-check",
    "build": "gatsby build",
    "develop": "gatsby develop --host 0.0.0.0",
    "gql-generate": "graphql-codegen --config graphql-codegen.yml",
    "gql-generate:watch": "yarn gql-generate --watch",
    "format": "prettier --write src/**/*.{js,jsx,json,ts,tsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "test": "react-scripts test",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xiel/xiel"
  },
  "bugs": {
    "url": "https://github.com/xiel/xiel/issues"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "@graphql-codegen/cli": "^1.2.0",
    "@graphql-codegen/typescript-operations": "^1.2.0",
    "@testing-library/react": "^8.0.6",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.7",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.8",
    "gatsby": "^2.8.6",
    "gatsby-image": "^2.1.2",
    "gatsby-plugin-emotion": "^4.1.0",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-offline": "^2.1.1",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sharp": "^2.1.3",
    "gatsby-plugin-typescript": "^2.0.15",
    "gatsby-source-filesystem": "^2.0.38",
    "gatsby-transformer-json": "^2.1.11",
    "gatsby-transformer-sharp": "^2.1.21",
    "husky": "^2.4.0",
    "i18next": "^17.0.3",
    "normalize.css": "^8.0.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-i18next": "^10.11.0",
    "react-scripts": "^3.0.1",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.3",
    "use-location-state": "2.1.5"
  }
}
